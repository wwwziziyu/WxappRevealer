{
  "patterns": [
    {
      "name": "jwt_token",
      "regex": "\"([A-Za-z0-9_-]{20,}\\.[A-Za-z0-9_-]{20,}\\.[A-Za-z0-9_-]{20,})\""
    },
    {
      "name": "wechat_appid",
      "regex": "\"(wx[0-9a-f]{16})\""
    },
    {
      "name": "wechat_original_id",
      "regex": "\"(gh_[0-9A-Za-z]{16})\""
    },
    {
      "name": "wechat_app_secret",
      "regex": "\"([0-9a-f]{32})\""
    },
    {
      "name": "aws_access_key_id",
      "regex": "\"aws_access_key_id\"\\s*:\\s*\"(AKIA[0-9A-Z]{16})\""
    },
    {
      "name": "aws_secret_access_key",
      "regex": "\"aws_secret_access_key\"\\s*:\\s*\"([A-Za-z0-9/+=]{40})\""
    },
    {
      "name": "google_api_key",
      "regex": "\"(AIza[0-9A-Za-z_-]{35})\""
    },
    {
      "name": "slack_token",
      "regex": "\"(xox[baprs]-[A-Za-z0-9]{10,48})\""
    },
    {
      "name": "stripe_secret_key",
      "regex": "\"(sk_live_[0-9a-zA-Z_-]{24})\""
    },
    {
      "name": "mongodb_uri",
      "regex": "\"(mongodb(?:\\+srv)?:\\/\\/(?:[^:@\\s]+:[^:@\\s]+@)?[A-Za-z0-9\\.\\-]+\\/[A-Za-z0-9_\\-\\.]+(?:\\?[^\\s]+)?)\""
    },
    {
      "name": "redis_url",
      "regex": "\"(redis:\\/\\/(?:[^:@\\s]+:[^:@\\s]+@)?[A-Za-z0-9\\.\\-]+:\\d{2,5})\""
    },
    {
      "name": "rsa_private_key",
      "regex": "-----BEGIN RSA PRIVATE KEY-----(?:[\\s\\S]+?)-----END RSA PRIVATE KEY-----"
    },
    {
      "name": "ssh_private_key",
      "regex": "-----BEGIN OPENSSH PRIVATE KEY-----(?:[\\s\\S]+?)-----END OPENSSH PRIVATE KEY-----"
    }
  ]
}